{% extends 'baseAdmin.html.twig' %}

{% block body %}

    <div id="haut" class="container-fluid">
        <h1 class="text-center title-boutique"> Mes Partenaires</h1>
        {% for partner in partners %}
            <div class="row">
                <div class="col-md-offset-4 col-md-4 text-center">
                    <a href="{{ path('partners_edit', { 'id': partner.id }) }}" class="thumbnail">
                        <img class="img-responsive"
                             src="{{ asset( 'uploads/partner/' ~ partner.imageName) }}"/>
                    </a>
                </div>
            </div>
        {% endfor %}
    </div>
{% endblock %}
